{
  "expo": {
    "name": "careMinder-app",
    "slug": "careMinder-app",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/CMicon.png",
    "scheme": "myapp",
    "backgroundColor": "#ffffff",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mycompany.careminder"  
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}